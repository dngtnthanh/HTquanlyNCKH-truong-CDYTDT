
@{
    Layout = null;
}

<!DOCTYPE html>
@model HTquanlyNCKH.Models.Degree
@using (Html.BeginForm("DegreeStoreOrEdit", "Scientist", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.degreeID)
    @Html.HiddenFor(model => model.degCreateDate)
    @Html.HiddenFor(model => model.degModifierDate)

    @Html.HiddenFor(model => model.degCreateUser)
    @Html.HiddenFor(model => model.degModifierUser)

    <div class="form-group">
        @Html.LabelFor(model => model.degName, "Tên trình độ", new { @class = "control-label" })
        @Html.EditorFor(model => model.degName, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.degName)
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.degDescription, "Mô tả", new { @class = "control-label" })
        @Html.EditorFor(model => model.degDescription, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.degDescription)
    </div>
    <div class="form-group">

        <input type="submit" value="Lưu lại" class="btn btn-success " />
        <input type="reset" value="Làm mới" class="btn btn-warning" />
    </div>
}  